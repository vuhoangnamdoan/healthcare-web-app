# Database Credentials (used by Docker Compose)
POSTGRES_DB=healthdb
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres123

# Application Settings (used by the 'backend' service)
DEBUG=False
DATABASE_URL=postgresql://postgres:postgres123@postgres:5432/healthdb
# Note: Since 'backend' depends on 'postgres' and they are on the same 
# docker network, 'postgres' is the correct hostname.
